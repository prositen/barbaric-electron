{% include 'header.html.j2' %}
<table class="table table-hover" id="table-users">
<thead>
    <tr>
        <th>Email</th>
        <th>Admin</th>
    </tr>
</thead>
<tbody>
{% for u in users %}
    <tr>
        <td>{{ u.email }}</td>
        <td>{{ u.admin }}</td>
    </tr>
{% endfor %}
</tbody>
</table>

